<!DOCTYPE mapper PUBLIC "-//mybatis.org//DTD Mapper 3.0//EN" "http://mybatis.org/dtd/mybatis-3-mapper.dtd" >
<mapper namespace="zy.com.cn.demo.take.task.mapper.UserMapper">
    <sql id="BASE_TABLE">
      task_user
    </sql>
    <sql id="BASE_COLUMN">
        id,name,mobile,created_by,created_date,updated_by,updated_date,deleted_flag
    </sql>
    <insert id="insert" parameterType="zy.com.cn.demo.take.task.model.User">
        insert  into
        <include refid="BASE_TABLE"/>
        <trim prefix="(" suffix=")" suffixOverrides=",">
            name, mobile, created_by, created_date,updated_by,updated_date,
        </trim>
        <trim prefix="VALUES(" suffix=")" suffixOverrides=",">
            #{name, jdbcType=VARCHAR},
            #{mobile, jdbcType=VARCHAR},
            #{createdBy, jdbcType=VARCHAR},
            NOW(),
            #{updatedBy,jdbcType=VARCHAR},
            NOW(),
        </trim>
    </insert>
</mapper>